<form_pane>
<input type="hidden" name="oid[]" value="" />
<fieldset class="adminform">
    <legend>Образование</legend>
    <table cellspacing="1" class="admintable">
        <tbody>
        <tr>
            <td width="185" valign="top" class="key">
                Учебное заведение
            </td>
            <td>
                <input type="hidden" id="учебное_заведение" name="учебное_заведение" value="" class="required"/>
                <справочник вид="дерево" имя="dic_education_institution" name="учебное_заведение" />
            </td>
        </tr>
        <tr>
            <td width="185" valign="top" class="key">
                Год окончания
            </td>
            <td>
                <input type="text" id="год_окончания" name="год_окончания" style="width: 100px; height: 17px;" value="" class="required" />
            </td>
        </tr>
        <tr>
            <td width="185" valign="top" class="key">
                Серия диплома
            </td>
            <td>
                <input type="text" id="серия_диплома" name="серия_диплома" style="width: 200px; height: 17px;" value="" class="required" />
            </td>
        </tr>
        <tr>
            <td width="185" valign="top" class="key">
                Номер диплома
            </td>
            <td>
                <input type="text" id="номер_диплома" name="номер_диплома" style="width: 200px; height: 17px;" value="" class="required" />
            </td>
        </tr>        
        <tr>
            <td width="185" valign="top" class="key">
                Специальность
            </td>
            <td>
                <input type="hidden" id="специальность" name="специальность" value="" class="required"/>
                <справочник вид="дерево" имя="dic_education_specialities" name="специальность" />
            </td>
        </tr>
        <tr>
            <td valign="top" class="key">
                Тип образования
            </td>
            <td>
                <справочник вид="список" имя="dic_education_type" name="тип_образования" проверка="required" />
            </td>
        </tr>
        </tbody>
    </table>
</fieldset>
</form_pane>